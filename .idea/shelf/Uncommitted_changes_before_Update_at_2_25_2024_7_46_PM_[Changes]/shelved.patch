<+>{% extends 'template.html.twig' %}\r\n{% block title %}Log in!{% endblock %}\r\n{% block pageTitle %} <center> Log in </center>{% endblock %}\r\n{% block navbar_search %}{% endblock %}\r\n{% block navbar_button %}{% endblock %}\r\n{% block layoutSidenav_nav %}{% endblock %}\r\n{% block Sidebar_toggle %}{% endblock %}\r\n\r\n{% block body %}\r\n    <div class=\"container mt-5\">\r\n        <div class=\"row justify-content-center\">\r\n            <div class=\"col-md-6\">\r\n                <form method=\"post\" class=\"card p-4 shadow\">\r\n                    <div class=\"mb-3 text-center\">\r\n                        <img src=\"{{ asset('assets/img/04.png') }}\" alt=\"Logo\" class=\"img-fluid mb-4\" style=\"max-width: 250px;\">\r\n                    </div>\r\n\r\n                    {% if error %}\r\n                        <div class=\"alert alert-danger\">{{ error.messageKey|trans(error.messageData, 'security') }}</div>\r\n                    {% endif %}\r\n\r\n                    {% if app.user %}\r\n                        <div class=\"mb-3\">\r\n                            You are logged in as {{ app.user.userIdentifier }}, <a href=\"{{ path('app_logout') }}\">Logout</a>\r\n                        </div>\r\n                    {% endif %}\r\n\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"inputEmail\" class=\"form-label\">Email address</label>\r\n                        <input type=\"email\" value=\"{{ last_username }}\" name=\"email\" id=\"inputEmail\" class=\"form-control\" autocomplete=\"email\" required autofocus>\r\n                    </div>\r\n\r\n                    <div class=\"mb-3\">\r\n                        <label for=\"inputPassword\" class=\"form-label\">Password</label>\r\n                        <input type=\"password\" name=\"password\" id=\"inputPassword\" class=\"form-control\" autocomplete=\"current-password\" required>\r\n                        <input type=\"hidden\" name=\"_csrf_token\" value=\"{{ csrf_token('authenticate') }}\">\r\n                    </div>\r\n\r\n                    <button type=\"submit\" class=\"btn btn-primary\">Sign in</button>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    {% block footer %}{% endblock %}\r\n{% endblock %}\r\n
